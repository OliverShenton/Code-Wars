public class vowelCount {

  public static int vowelCount(String word) {
  
    int vowelCount = 0;
    
    for (int i = 0; i < word.length(); i++) {
      char c = word.charAt(i);
      if (c == 'a' || c == 'e' || c == 'i' || c == 'o' || c == 'u') {
        vowelCount ++;
       }
    }
    return vowelCount;
  }
}
